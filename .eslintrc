{
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-uses-react": "off",
      "semi": [2, "always"]
    }
}

